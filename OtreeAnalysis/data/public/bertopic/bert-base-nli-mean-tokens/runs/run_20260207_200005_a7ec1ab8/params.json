{
  "embed_model_outname": "bert-base-nli-mean-tokens",
  "embed_hf": "sentence-transformers/bert-base-nli-mean-tokens",
  "prefix": null,
  "encode_normalize": true,
  "vectorizer": {
    "stop_words": "english",
    "ngram_range": [
      1,
      3
    ],
    "min_df": 1
  },
  "umap": {
    "n_neighbors": 45,
    "n_components": 2,
    "min_dist": 0.1
  },
  "hdbscan": {
    "min_cluster_size": 75,
    "min_samples": 5
  },
  "random_state": 42
}